<!--begin::Aside Menu-->
<div class="hover-scroll-overlay-y"
     id="kt_aside_menu_wrapper"
     data-kt-scroll="true"
     data-kt-scroll-activate="{default: false, lg: true}"
     data-kt-scroll-height="auto"
     data-kt-scroll-dependencies="#kt_aside_logo, #kt_aside_footer"
     data-kt-scroll-wrappers="#kt_aside_menu"
     data-kt-scroll-offset="0">

    <!--begin:: LOCATION Menu-->
    <div class="menu menu-column menu-rounded menu-sub-indention fw-semibold fs-6"
         id="#kt_app_sidebar_menu_2"
         data-kt-menu="true"
         data-kt-menu-expand="false">
        <!--begin:Menu item-->
        <div data-kt-menu-trigger="click" class="menu-item here  menu-accordion">
            <!--begin:Menu link-->
            <span class="menu-link">
                <span class="menu-icon">
                    <i class="fas fa-map-marker-alt fs-2"></i>
                </span><span class="menu-title">Location</span><span class="menu-arrow"></span>
            </span>
            <!--end:Menu link-->

            <!--begin:Menu sub-->
            <div class="menu-sub menu-sub-accordion">
                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* BRANCH *@
                    <a class="menu-link" asp-controller="GenericTable" asp-action="Index" asp-route-modelName="Branch">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Branches</span>
                    </a>

                    <!--end:Menu link-->
                </div>
                <!--end:Menu item-->

                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* COUNTRY *@
                    <a class="menu-link" asp-controller="Generic" asp-action="Index" asp-route-modelName="Country">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Countries</span>
                    </a>
                </div>
                <!--end:Menu item-->

                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* LANGUAGE *@
                    <a class="menu-link" asp-controller="Languages" asp-action="Index">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Languages</span>
                    </a>
                    <!--end:Menu link-->
                </div>
                <!--end:Menu item-->

                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* PROVINCE *@
                    <a class="menu-link" asp-controller="Provinces" asp-action="Index">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Provinces</span>
                    </a>
                    <!--end:Menu link-->
                </div>
                <!--end:Menu item-->

                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* SUB PROVINCE *@
                    <a class="menu-link" asp-controller="SubProvinces" asp-action="Index">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Sub Provinces</span>
                    </a>
                    <!--end:Menu link-->
                </div>

            </div>
            <!--end:Menu sub-->
        </div>
        <!--end:Menu item-->
    </div>
    <!--end::Menu-->
    <!--begin:: KIOSK APPS Menu-->
    <div class="menu menu-column menu-rounded menu-sub-indention fw-semibold fs-6"
         id="#kt_app_sidebar_menu_3"
         data-kt-menu="true"
         data-kt-menu-expand="false">
        <!--begin:Menu item-->
        <div data-kt-menu-trigger="click" class="menu-item here  menu-accordion">
            <!--begin:Menu link-->
            <span class="menu-link">
                <span class="menu-icon">
                    <i class="fas fa-desktop fs-2"></i>
                </span><span class="menu-title">Kiosk Apps</span><span class="menu-arrow"></span>
            </span>
            <!--end:Menu link-->

            <!--begin:Menu sub-->
            <div class="menu-sub menu-sub-accordion">
                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* KAPP SETTINGS *@
                    <a class="menu-link" asp-controller="KappSettings" asp-action="Index">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Kapp Settings</span>
                    </a>
                    <!--end:Menu link-->
                </div>
                <!--end:Menu item-->

                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* KIOSK APPLICATION *@
                    <a class="menu-link" asp-controller="KioskApplication" asp-action="Index">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Kiosk Application</span>
                    </a>
                </div>
                <!--end:Menu item--

                ><!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* KIOSK APPLICATION TYPE *@
                    <a class="menu-link" asp-controller="KioskApplicationType" asp-action="Index">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Kiosk Application Type</span>
                    </a>
                    <!--end:Menu link-->
                </div>
                <!--end:Menu item-->

                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* KAPP WORKFLOW *@
                    <a class="menu-link">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Kapp Workflow</span>
                    </a>
                    <!--end:Menu link-->
                </div>
                <!--end:Menu item-->

                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* MONITORING *@
                    <a class="menu-link" asp-controller="Monitoring" asp-action="Index">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Monitoring</span>
                    </a>
                    <!--end:Menu link-->
                </div>
                <!--end:Menu item-->

                <!--begin:Menu item-->
                <div class="menu-item">
                    <!--begin:Menu link-->
                    @* RESOURCE *@
                    <a class="menu-link" asp-controller="Resources" asp-action="Index">
                        <span class="menu-bullet">
                            <span class="bullet bullet-dot"></span>
                        </span>
                        <span class="menu-title">Resources</span>
                    </a>
                    <!--end:Menu link-->
                </div>
                <!--end:Menu item-->

            </div>
            <!--end:Menu sub-->
        </div>
        <!--end:Menu item-->
    </div>
    <!--end::Menu-->
</div>